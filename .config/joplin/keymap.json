[
  { "keys": ["l", "CTRL_L", "CTRL_Wl"], "type": "function", "command": "focus_next" },
  { "keys": ["h", "CTRL_H", "CTRL_Wh"], "type": "function", "command": "focus_previous" },
  { "keys": ["k", "CTRL_K", "CTRL_Wk"], "type": "function", "command": "move_up" },
  { "keys": ["j", "CTRL_J", "CTRL_Wj"], "type": "function", "command": "move_down" },
  { "keys": ["u"], "type": "function", "command": "page_up" },
  { "keys": ["d"], "type": "function", "command": "page_down" },
  { "keys": ["/"], "type": "prompt", "command": "search \"\"", "cursorPosition": -2 },

  { "keys": ["ENTER", "o"], "type": "function", "command": "activate" },
  { "keys": ["ma"], "type": "prompt", "command": "mknote \"\"", "cursorPosition": -2 },
  { "keys": ["mb"], "type": "prompt", "command": "mkbook \"\"", "cursorPosition": -2 },
  { "keys": ["md"], "type": "function", "command": "delete" },
  { "keys": ["mm"], "type": "prompt", "command": "ren $n \"\"", "cursorPosition": -2 },
  { "keys": ["mt"], "type": "prompt", "command": "mktodo \"\"", "cursorPosition": -2 },
  { "keys": ["mv"], "type": "prompt", "command": "mv $n \"\"", "cursorPosition": -2 },

  { "keys": [":"], "type": "function", "command": "enter_command_line_mode" },
  { "keys": ["tt", "tc"], "type": "function", "command": "toggle_console" },
  { "keys": ["tm"], "type": "function", "command": "toggle_metadata" },
  { "keys": ["yy", "yn"], "type": "prompt", "command": "cp $n \"\"", "cursorPosition": -2 },
  { "keys": ["qt", "gt"], "type": "prompt", "command": "tag add \"\" $n", "cursorPosition": 9 },
  { "keys": ["gs"], "type": "prompt", "command": "config notes.sortOrder.field"},
  { "keys": ["q", "ZQ", "ZZ"], "type": "exec", "command": "exit" },
  { "keys": ["x"], "command": "todo toggle $n" }
]
